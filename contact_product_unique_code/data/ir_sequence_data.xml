<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequences for customer -->
        <record id="sequence_for_customer" model="ir.sequence">
            <field name="name">customer sequence</field>
            <field name="code">customer.sequence</field>
            <field name="active">TRUE</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="prefix">CUS</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Sequences for supplier -->
        <record id="sequence_for_supplier" model="ir.sequence">
            <field name="name">supplier sequence</field>
            <field name="code">supplier.sequence</field>
            <field name="active">TRUE</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="prefix">SUP</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Sequences for product -->
        <record id="sequence_for_product" model="ir.sequence">
            <field name="name">product sequence</field>
            <field name="code">product.sequence</field>
            <field name="active">TRUE</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="prefix">PRO</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>

<?xml version='1.0' encoding='utf-8'?>
<odoo>

    <!-- contacts form inherit -->
	<record id="view_partner_form_contact_inherit" model="ir.ui.view">
        <field name="name">view.partner.form.inharit.contact.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="after">
                <field name="customer_rank"/>
            </xpath>
            <xpath expr="//field[@name='customer_rank']" position="after">
                <field name="supplier_rank"/>
            </xpath>
        </field>
    </record>

</odoo>
